; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Base Project"
run/main_scene="res://miscellaneous/first_boot/intro.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
boot_splash/image="res://miscellaneous/first_boot/bootsplash.png"
config/icon="res://icon.svg"
boot_splash/minimum_display_time=300

[autoload]

SGlobal="*res://logic/data/SGlobal.gd"

[editor_plugins]

enabled=PackedStringArray("res://addons/zl_init_plugin/plugin.cfg")

[file_customization]

folder_colors={
"res://addons/": "purple",
"res://assets/": "pink",
"res://characters/": "yellow",
"res://logic/": "teal",
"res://world/": "orange"
}

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
